{
    "compilerOptions": {
        "target": "ES2024",
        "module": "Node16",
        "moduleResolution": "node16",
        "noEmit": true,
        "strict": true,
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "sourceMap": true,
    },
    "include": [
        "."
    ]
}

